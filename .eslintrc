{
  parser: "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
  	"max-len": [1, 100, 2, {ignoreComments: true}],
  	"linebreak-style": ["error", "windows"],
  	"class-methods-use-this": 0,
  	"import/extensions": 0,
  	"react/jsx-uses-react": "error",
  	"react/jsx-uses-vars": 2,
  	"import/no-extraneous-dependencies": 0,
  	"object-curly-newline": 0,
  	"no-undef": 0,
  	"no-alert": 0,
  },
  "extends": [
  	"airbnb-base"
  ]
}